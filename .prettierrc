{
  "endOfLine": "lf",
  "importOrder": [
    "^react$|^react-native$",
    "^react",
    "^@screens",
    "^@components",
    "^@/redux",
    "^@/navigator|@react-navigation",
    "^@/hook|^@/utils",
    "^@/type",
    "^@/.*|^@.*",
    "^[^.].*",
    "^..?/"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true,
  "arrowParens": "always",
  "bracketSpacing": true,
  "embeddedLanguageFormatting": "auto",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "printWidth": 80,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "vueIndentScriptAndStyle": false
}
